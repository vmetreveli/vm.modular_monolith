{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Port=5433;Database=ModularMonolithDb;User Id=postgres;Password=postgres;"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
//      "Serilog.Sinks.Elasticsearch"
    ],
    "MinimumLevel": "Debug",
    "Enrich": [
      "WithMachineName",
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "AllowedHosts": "*",
  "AppConfiguration": {
    "RabbitMQ": {
      "ExchangeName": "MyMqExchange",
      "Host": "rabbitmq",
      "VirtualHost": "dev",
      "Username": "modular",
      "Password": "modular"
    },
    "Quartz": {
      "OutboxJob": "0/3 * * * * ?"
    },
    "ExternalServices": {
      "CatalogApi": {
        "BaseAddress": "https://localhost:5729/api/v1/"
      }
    }
  }
}